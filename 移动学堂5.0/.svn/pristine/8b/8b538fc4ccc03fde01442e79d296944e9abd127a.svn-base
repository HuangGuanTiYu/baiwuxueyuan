#import "BusinessTypeTool.h"

@implementation BusinessTypeTool

+ (NSString *) returnBusinessTypeWithCode : (NSString *) code
{
    if ([code isEqualToString:courseType]) {
        return @"课程";
    }else if ([code isEqualToString:liveType])
    {
        return @"直播";
    }else if ([code isEqualToString:shalongType])
    {
        return @"沙龙";
    }else if ([code isEqualToString:specialType])
    {
        return @"专题";
    }else if ([code isEqualToString:testType])
    {
        return @"考试";
    }else if ([code isEqualToString:voteType])
    {
        return @"问卷";
    }else if ([code isEqualToString:newsType])
    {
        return @"资讯";
    }else if ([code isEqualToString:teacherType])
    {
        return @"讲师";
    }
    
    return @"";
}

@end
